				{
					"name": "Other_Service_Order",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"// —————————————————————————————————————\r",
									"// CONFIGURE THESE TO MATCH SEED DATA\r",
									"// —————————————————————————————————————\r",
									"const otherServiceIds  = [2,3,4,5,6];\r",
									"const footageSizeIds   = [1,2,3,4];\r",
									"\r",
									"const streetAddresses = [\r",
									"  \"123 Main St\", \"456 Oak Avenue\", \"789 Pine Lane\",\r",
									"  \"321 Elm Blvd\", \"654 Maple Court\", \"987 Cedar Way\"\r",
									"];\r",
									"const cities = [\"Springfield\",\"Fairview\",\"Riverside\",\"Greenville\",\"Madison\"];\r",
									"const states = [\"CA\",\"NY\",\"TX\",\"FL\",\"IL\"];\r",
									"const zipCodes = [\"90210\",\"10001\",\"60614\",\"33101\",\"73301\"];\r",
									"\r",
									"// ————————————————————————————————\r",
									"// PROFESSIONAL NAMES & CONTACT\r",
									"// ————————————————————————————————\r",
									"const firstNames = [\r",
									"  \"Alexander\",\"Victoria\",\"Christopher\",\"Elizabeth\",\r",
									"  \"Nicholas\",\"Catherine\",\"Jonathan\",\"Margaret\"\r",
									"];\r",
									"const lastNames = [\r",
									"  \"Thompson\",\"Richardson\",\"Patterson\",\"Henderson\",\r",
									"  \"Morrison\",\"Cooper\",\"Stewart\",\"Turner\"\r",
									"];\r",
									"const domains = [\r",
									"  \"gmail.com\",\"outlook.com\",\"yahoo.com\",\r",
									"  \"company.com\",\"business.com\",\"realty.com\"\r",
									"];\r",
									"\r",
									"const firstName = firstNames[Math.floor(Math.random()*firstNames.length)];\r",
									"const lastName  = lastNames[Math.floor(Math.random()*lastNames.length)];\r",
									"const rndNum    = Math.floor(Math.random()*900)+100;\r",
									"const domain    = domains[Math.floor(Math.random()*domains.length)];\r",
									"const email     = `${firstName.toLowerCase()}.${lastName.toLowerCase()}${rndNum}@${domain}`;\r",
									"\r",
									"// US phone (avoid leading 0/1 in area code)\r",
									"const area = Math.floor(200 + Math.random()*799);\r",
									"const exch = Math.floor(200 + Math.random()*799);\r",
									"const last4 = Math.floor(1000 + Math.random()*8999);\r",
									"const phone = `+1${area}${exch}${last4}`;\r",
									"\r",
									"// ————————————————————————\r",
									"// ADDITIONAL INFO MESSAGE\r",
									"// ————————————————————————\r",
									"const messages = [\r",
									"  \"Hello! I'm a real estate professional seeking your photography and staging services.\",\r",
									"  \"Good morning! I represent property owners and need your drone & photo packages.\",\r",
									"  \"Hi there! Looking for a reliable partner for property marketing services.\",\r",
									"  \"Hello! Would like to establish a business relationship for regular property shoots.\"\r",
									"];\r",
									"const additionalInfo = messages[Math.floor(Math.random()*messages.length)];\r",
									"\r",
									"// ———————————————————————————\r",
									"// PICK RANDOM ENUMS & ADDRESS\r",
									"// ———————————————————————————\r",
									"const otherServiceId = otherServiceIds[Math.floor(Math.random()*otherServiceIds.length)];\r",
									"const footageSizeId  = footageSizeIds[Math.floor(Math.random()*footageSizeIds.length)];\r",
									"const address        = streetAddresses[Math.floor(Math.random()*streetAddresses.length)];\r",
									"const city           = cities[Math.floor(Math.random()*cities.length)];\r",
									"const state          = states[Math.floor(Math.random()*states.length)];\r",
									"const zipCode        = zipCodes[Math.floor(Math.random()*zipCodes.length)];\r",
									"\r",
									"// ——————————————————————\r",
									"// SET POSTMAN VARIABLES\r",
									"// ——————————————————————\r",
									"pm.variables.set(\"first_name\",      firstName);\r",
									"pm.variables.set(\"last_name\",       lastName);\r",
									"pm.variables.set(\"email\",           email);\r",
									"pm.variables.set(\"phone_number\",    phone);\r",
									"pm.variables.set(\"other_services_id\", otherServiceId);\r",
									"pm.variables.set(\"additional_info\", additionalInfo);\r",
									"pm.variables.set(\"address\",         address);\r",
									"pm.variables.set(\"city\",            city);\r",
									"pm.variables.set(\"state\",           state);\r",
									"pm.variables.set(\"zip_code\",        zipCode);\r",
									"pm.variables.set(\"footage_size_id\", footageSizeId);\r",
									"\r",
									"// ——————————————————————\r",
									"// LOG FOR VERIFICATION\r",
									"// ——————————————————————\r",
									"console.log(\"Order payload vars:\");\r",
									"console.log({ firstName, lastName, email, phone, otherServiceId, additionalInfo, address, city, state, zipCode, footageSizeId });\r",
									""
								],
